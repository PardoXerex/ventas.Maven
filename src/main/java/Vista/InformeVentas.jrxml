	<queryString language="SQL">
		<![CDATA[SELECT
     config.`telefono` AS config_telefono,
     config.`direccion` AS config_direccion,
     ventas.`id` AS ventas_id,
     ventas.`total` AS ventas_total,
     ventas.`fecha` AS ventas_fecha,
     ventas.`cliente` AS ventas_cliente,
     detalle.`id_pro` AS detalle_id_pro,
     detalle.`cantidad` AS detalle_cantidad,
     detalle.`precio` AS detalle_precio,
     detalle.`id_venta` AS detalle_id_venta,
     clientes.`dni` AS clientes_dni,
     clientes.`nombre` AS clientes_nombre,
     productos.`nombre` AS productos_nombre
FROM
     `ventas` ventas INNER JOIN `detalle` detalle ON ventas.`id` = detalle.`id_venta`
     INNER JOIN `clientes` clientes ON ventas.`cliente` = clientes.`id`
     INNER JOIN `productos` productos ON detalle.`id_pro` = productos.`id`,
     `config` config]]>
	</queryString>
